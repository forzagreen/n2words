{
  "name": "n2words",
  "version": "1.11.0",
  "description": "Convert numbers to words, in multiple languages",
  "main": "dist/n2words.js",
  "scripts": {
    "lint": "eslint --config .eslintrc.json lib/ test/",
    "test": "ava --verbose",
    "coverage": "c8 ava",
    "build": "webpack --config webpack.config.js --progress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forzagreen/n2words.git"
  },
  "keywords": [
    "n2words",
    "convert",
    "number",
    "word",
    "text",
    "natural",
    "language",
    "english",
    "french",
    "spanish",
    "german",
    "portuguese",
    "italian",
    "turkish",
    "russian",
    "czech",
    "norwegian",
    "danish",
    "polish",
    "ukrainian",
    "lithuanian",
    "latvian",
    "arabic",
    "hebrew",
    "korean",
    "dutch",
    "serbian",
    "farsi",
    "persian",
    "chinese",
    "hungarian",
    "indonesian",
    "croatian"
  ],
  "author": "Wael TELLAT",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/forzagreen/n2words/issues"
  },
  "homepage": "https://github.com/forzagreen/n2words#readme",
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/preset-env": "^7.16.11",
    "ava": "^4.1.0",
    "babel-loader": "^8.2.3",
    "c8": "^7.11.0",
    "core-js": "^3.21.1",
    "eslint": "^8.11.0",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^38.0.4",
    "eslint-plugin-node": "^11.1.0",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "ava": {
    "files": [
      "test/**/*",
      "!test/i18n"
    ]
  },
  "c8": {
    "all": true,
    "include": [
      "lib/"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
