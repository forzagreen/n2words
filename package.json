{
  "name": "n2words",
  "version": "1.13.0",
  "description": "Convert numbers to words, in multiple languages",
  "main": "dist/n2words.js",
  "scripts": {
    "lint": "eslint --config .eslintrc.json lib/ test/",
    "test": "ava --verbose",
    "test:en": "npm run test -- -- --lang en",
    "coverage": "c8 ava",
    "build": "webpack --config webpack.config.js --progress",
    "benchmark": "node bench.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forzagreen/n2words.git"
  },
  "engines": {
    "node": "16 || >=18"
  },
  "keywords": [
    "n2words",
    "convert",
    "number",
    "word",
    "text",
    "natural",
    "language",
    "english",
    "french",
    "spanish",
    "german",
    "portuguese",
    "italian",
    "turkish",
    "russian",
    "czech",
    "norwegian",
    "danish",
    "polish",
    "ukrainian",
    "lithuanian",
    "latvian",
    "arabic",
    "hebrew",
    "korean",
    "dutch",
    "serbian",
    "farsi",
    "persian",
    "chinese",
    "hungarian",
    "indonesian",
    "croatian",
    "vietnamese",
    "azerbaijani"
  ],
  "author": "Wael TELLAT",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/forzagreen/n2words/issues"
  },
  "homepage": "https://github.com/forzagreen/n2words#readme",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "ava": "^5.2.0",
    "babel-loader": "^9.1.2",
    "benchmark": "^2.1.4",
    "c8": "^7.13.0",
    "core-js": "^3.30.2",
    "eslint": "^8.40.0",
    "eslint-plugin-ava": "^14.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^44.2.3",
    "eslint-plugin-node": "^11.1.0",
    "microtime": "^3.1.1",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1"
  },
  "ava": {
    "files": [
      "test/**/*",
      "!test/i18n"
    ]
  },
  "c8": {
    "all": true,
    "include": [
      "lib/"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  }
}
