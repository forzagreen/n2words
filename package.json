{
  "name": "n2words",
  "version": "2.0.0",
  "description": "n2words converts a numerical number into a written one, supports 47 languages and has zero dependencies.",
  "keywords": [
    "n2words",
    "number-to-words",
    "cardinal",
    "spellout",
    "conversion",
    "internationalization",
    "localization",
    "multilingual",
    "pluralization",
    "typescript",
    "browser",
    "nodejs",
    "zero-dependencies",
    "bigint",
    "formatting",
    "utility"
  ],
  "homepage": "https://github.com/forzagreen/n2words#readme",
  "bugs": {
    "url": "https://github.com/forzagreen/n2words/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forzagreen/n2words.git"
  },
  "license": "MIT",
  "author": "Wael TELLAT",
  "contributors": [
    "Tyler Vigario (https://github.com/TylerVigario)"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/n2words.js",
      "types": "./typings/n2words.d.ts"
    },
    "./languages/*": {
      "import": "./lib/languages/*.js",
      "types": "./typings/languages/*.d.ts"
    }
  },
  "main": "lib/n2words.js",
  "jsdelivr": "dist/n2words.js",
  "unpkg": "dist/n2words.js",
  "types": "typings/n2words.d.ts",
  "typesVersions": {
    "*": {
      "n2words/*": [
        "typings/*"
      ]
    }
  },
  "files": [
    "lib/*",
    "dist/*",
    "typings/*"
  ],
  "scripts": {
    "bench:perf": "node bench.js",
    "bench:memory": "node --expose-gc bench-memory.js",
    "clean": "npm run types:clean && npm run web:clean && npm run docs:clean && npm run coverage:clean",
    "coverage:clean": "node -e \"import { rmSync } from 'fs'; try { rmSync('coverage', { recursive: true, force: true }) } catch (e) {}\"",
    "coverage:generate": "npm run coverage:clean && c8 npm run test",
    "docs:generate": "npm run docs:clean && jsdoc -c ./conf.json && npm run docs:validate",
    "docs:clean": "node -e \"import { rmSync } from 'fs'; try { rmSync('docs', { recursive: true, force: true }) } catch (e) {}\"",
    "lang:add": "node scripts/add-language.js",
    "lang:validate": "node scripts/validate-language.js",
    "lint": "npm run lint:js && npm run lint:md",
    "lint:js": "standard examples/ lib/ scripts/ test/ *.js",
    "lint:md": "markdownlint-cli2 *.md guides/*.md scripts/*.md",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "ava --verbose test/integration/*.{js,cjs}",
    "test:unit": "ava --verbose test/unit/*.js",
    "types:generate": "npm run types:clean && tsc --project tsconfig.json",
    "types:clean": "node -e \"import { rmSync } from 'fs'; try { rmSync('typings', { recursive: true, force: true }) } catch (e) {}\"",
    "types:validate": "npm run types:build && tsc --noEmit --project test/typescript/tsconfig.json && node --import tsx --test test/typescript/*.ts",
    "web:build": "npm run web:clean && rollup -c",
    "web:clean": "node -e \"import { rmSync } from 'fs'; try { rmSync('dist', { recursive: true, force: true }) } catch (e) {}\"",
    "web:test": "npm run web:build && ava --verbose test/web/*.js"
  },
  "ava": {
    "extensions": [
      "js",
      "cjs"
    ],
    "files": [
      "test/unit/*.js",
      "test/integration/*.{js,cjs}",
      "test/web/*.js"
    ],
    "timeout": "30s"
  },
  "c8": {
    "all": true,
    "include": [
      "lib/"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "ava": "^6.4.1",
    "benchmark": "^2.1.4",
    "c8": "^10.1.3",
    "chalk": "^5.6.2",
    "chromedriver": "^143.0.3",
    "core-js": "^3.47.0",
    "jsdoc": "^4.0.5",
    "markdownlint-cli2": "^0.20.0",
    "microtime": "^3.1.1",
    "rollup": "^4.28.1",
    "selenium-webdriver": "^4.39.0",
    "standard": "^17.1.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": "^20 || ^22 || >=24"
  }
}
