      "lib/"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.4",
    "ava": "^5.3.0",
    "babel-loader": "^9.1.2",
    "benchmark": "^2.1.4",
    "c8": "^7.14.0",
    "core-js": "^3.30.2",
    "eslint": "^8.41.0",
    "eslint-plugin-ava": "^14.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^46.0.0",
    "eslint-plugin-node": "^11.1.0",
    "microtime": "^3.1.1",
    "webpack": "^5.84.1",
    "webpack-cli": "^5.1.1"
  },
  "engines": {
    "node": "16 || >=18"
