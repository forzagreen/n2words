{
  "name": "n2words",
  "version": "1.23.2",
  "description": "n2words converts a numerical number into a written one, supports 38 languages and has zero dependencies.",
  "keywords": [
    "n2words",
    "convert",
    "number",
    "word",
    "text",
    "natural",
    "language",
    "i18n",
    "internationalization",
    "number-to-words",
    "numeral",
    "numeric",
    "cardinal",
    "spell-out",
    "english",
    "french",
    "spanish",
    "german",
    "portuguese",
    "italian",
    "turkish",
    "russian",
    "czech",
    "norwegian",
    "danish",
    "polish",
    "ukrainian",
    "lithuanian",
    "latvian",
    "arabic",
    "hebrew",
    "korean",
    "dutch",
    "serbian",
    "persian",
    "chinese",
    "hungarian",
    "indonesian",
    "croatian",
    "vietnamese",
    "azerbaijani",
    "japanese",
    "romanian",
    "malay",
    "bahasa melayu"
  ],
  "homepage": "https://github.com/forzagreen/n2words#readme",
  "bugs": {
    "url": "https://github.com/forzagreen/n2words/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forzagreen/n2words.git"
  },
  "license": "MIT",
  "author": "Wael TELLAT",
  "contributors": [
    "Tyler Vigario (https://github.com/TylerVigario)"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/n2words.js",
      "types": "./typings/n2words.d.ts"
    },
    "./i18n/*": {
      "import": "./lib/i18n/*.js",
      "types": "./typings/i18n/*.d.ts"
    }
  },
  "main": "lib/n2words.js",
  "jsdelivr": {
    "browser": "dist/n2words.js",
    "types": "typings/n2words.d.ts"
  },
  "unpkg": {
    "browser": "dist/n2words.js",
    "types": "typings/n2words.d.ts"
  },
  "types": "typings/n2words.d.ts",
  "typesVersions": {
    "*": {
      "n2words/*": [
        "typings/*"
      ]
    }
  },
  "files": [
    "lib/*",
    "dist/*",
    "typings/*"
  ],
  "scripts": {
    "bench": "node bench.js",
    "bench:memory": "node --expose-gc bench-memory.js",
    "build": "npm run build:web && npm run build:types",
    "build:types": "tsc --project tsconfig.json",
    "build:web": "webpack --progress",
    "coverage": "c8 npm run test:core",
    "docs": "npm run docs:clean && jsdoc -c ./conf.json && npm run docs:validate",
    "docs:clean": "node -e \"import { rmSync } from 'fs'; try { rmSync('docs', { recursive: true, force: true }) } catch (e) {}\"",
    "docs:validate": "echo 'Documentation generated successfully'",
    "lang:add": "node scripts/add-language.js",
    "lang:validate": "node scripts/validate-language.js",
    "lint": "npm run lint:js && npm run lint:md",
    "lint:js": "standard examples/ lib/ scripts/ test/ *.js",
    "lint:md": "markdownlint-cli2 *.md scripts/*.md",
    "test": "npm run test:core && npm run test:types",
    "test:core": "npm run test:unit && npm run test:integration && npm run test:smoke && npm run test:i18n",
    "test:i18n": "ava --verbose test/i18n.js",
    "test:integration": "ava --verbose test/integration/*.js",
    "test:smoke": "ava --verbose test/smoke/*.js",
    "test:types": "tsx test/typescript-smoke.ts",
    "test:unit": "ava --verbose test/unit/*.js",
    "test:web": "ava --verbose test/web.js"
  },
  "ava": {
    "extensions": [
      "js"
    ],
    "files": [
      "test/unit/*.js",
      "test/integration/*.js",
      "test/smoke/*.js",
      "test/i18n.js"
    ],
    "timeout": "30s"
  },
  "c8": {
    "all": true,
    "include": [
      "lib/"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "ava": "^6.4.1",
    "babel-loader": "^10.0.0",
    "benchmark": "^2.1.4",
    "c8": "^10.1.3",
    "chalk": "^5.6.2",
    "chromedriver": "^143.0.2",
    "core-js": "^3.47.0",
    "jsdoc": "^4.0.5",
    "markdownlint-cli2": "^0.20.0",
    "microtime": "^3.1.1",
    "selenium-webdriver": "^4.39.0",
    "standard": "^17.1.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "webpack": "^5.104.0",
    "webpack-cli": "^6.0.1"
  },
  "engines": {
    "node": "^20 || ^22 || >=24"
  }
}
