    "Tyler Vigario <TylerVigario90@gmail.com>"
  ],
  "type": "module",
  "exports": {
    ".": "./lib/n2words.js",
    "./i18n/": "./lib/i18n/"
  },
  "files": [
    "lib/*",
    "build/*"
  ],
  "scripts": {
    "bench": "node bench.js",
    "build": "webpack --config webpack.config.js --progress",
